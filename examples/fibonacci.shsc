module main

proc main()
    var inp = io:input("Enter a number: ", i64)
    var res = fibonacci(inp, {})
    io:print(f"result = {res}\n")
end

proc fibonacci(num, dp)
    assert:type(num, i64)
    assert:type(dp, map)
    if dp[num] then
        return dp[num]
    elif num <= 1 then
        return dp[num] = num
    else
        return dp[num] = fibonacci(num - 1, dp) + fibonacci(num - 2, dp)
    end
end
